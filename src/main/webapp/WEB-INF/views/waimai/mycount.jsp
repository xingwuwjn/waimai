<script>
    //删除我的时间
    function deleteMycount(id) {
        //获取openid
        var openid=$("input[name='openid']").val();
        $.ajax({
            type: "POST",
            url: "/waimai/deleteData",
            data: {
                id:id
            },
            success: function (data) {
                if(data=1){
                    $(".layercart").html("删除成功！");
                    $(".layercart").show();
                    setTimeout("window.location.href='/waimai/mycount?openid=abc'", 1500);

                }

            }
        });
    }
</script>
